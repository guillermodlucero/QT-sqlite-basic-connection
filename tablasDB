CREATE TABLE user (
 id integer PRIMARY KEY AUTOINCREMENT,
 user int NOT NULL,
 active boolean NOT NULL
);

INSERT INTO user(user, active) VALUES (1,1);
INSERT INTO user(user, active) VALUES (2,1);
INSERT INTO user(user, active) VALUES (3,1);

SELECT * FROM user

